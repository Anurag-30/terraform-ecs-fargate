
 [
  {
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "ecs-demo",
            "awslogs-region": "ap-south-1",
            "awslogs-create-group": "false",
            "awslogs-stream-prefix": "firelens"
        }       
    },
    "cpu": 10,
    "environment": [
      {"name": "VARNAME", "value": "VARVAL"}
    ],
    "essential": true,
    "image": "nginx",
    "memory": 128,
    "name": "nginx",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ]
  }
 ]